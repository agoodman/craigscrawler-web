%tr
	%td= (region=Region.find_by_code(feed.region)).title unless region.nil?
	%td= (category=Category.find_by_code(feed.category)).title unless category.nil?
	%td
		%ul
			= render :partial => 'feeds/keywords/keyword', :collection => feed.keywords, :locals => { :delete => false, :feed => feed }
	%td
		= link_to 'edit', feed, :class => 'button wrench'
		= link_to 'delete', feed, :method => :delete, :confirm => 'Are you sure you want to delete this feed? There is no undo.', :class => 'button trash'
